
<h1>New Post</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for @post do |f| %>
        <%= f.label :title %>
        <%= f.text_field :title %>

        <%= f.label :body %>
        <%= f.text_area :body %>

        <%= f.submit 'Create post', class: "btn btn-primary" %>
    <% end %>
  </div>
</div>